{
  "name": "scito/laravel-keycloak-admin",
  "description": "Laravel wrapper for the keycloak admin library",
  "keywords": [
    "laravel",
    "keycloak"
  ],
  "homepage": "https://gitlab.com/scito/laravel-keycloak-admin",
  "license": "MIT",
  "authors": [
    {
      "name": "Maikel van Maurik"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "scito/keycloak-admin": "~1.0.0"
  },
  "autoload": {
    "psr-4": {
      "Scito\\Laravel\\Keycloak\\Admin\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Scito\\Laravel\\Keycloak\\Admin\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Scito\\Laravel\\Keycloak\\Admin\\KeycloakServiceProvider"
      ],
      "aliases": {
        "KeycloakAdmin": "Scito\\Laravel\\Keycloak\\Admin\\Facades\\Keycloak"
      }
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^7.3@dev",
    "orchestra/testbench": "^3.7@dev",
    "squizlabs/php_codesniffer": "^3.4",
    "laravel/dusk": "^6.0@dev",
    "symfony/filesystem": "^4.3@dev",
    "symfony/finder": "^4.3@dev"
  }
}
